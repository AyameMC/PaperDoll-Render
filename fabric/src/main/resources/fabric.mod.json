{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Lucun_Ji(Original author)",
    "HappyRespawnanchor"
  ],
  "contributors": [
    "Fallen_Breath",
    "X-Green",
    "altrisi",
    "plusls",
    "pertaz"
  ],
  "contact": {
    "homepage": "${mod_homepage_url}",
    "sources": "${mod_fabric_sources_url}",
    "issues": "${mod_issue_url}"
  },
  "license": "${mod_license}",
  "icon": "assets/ayame_paperdoll/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.ayamemc.ayamepaperdoll.fabric.AyamePaperDollFabric"
    ],
    "modmenu": [
      "org.ayamemc.ayamepaperdoll.fabric.AyamePaperDollModMenuApiImpl"
    ]
  },
  "mixins": [
    "${mod_mixin_config}"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "{fabric_minecraft_version_range}",
    "fabric-api": "*"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/ayame-paperdoll",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/ayame-paperdoll"
      }
    }
  }
}